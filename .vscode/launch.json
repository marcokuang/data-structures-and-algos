  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Tree",
      "program": "${workspaceFolder}//Algorithms/Tree/boggleBoard.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    },
    {
      "name": "Launch List",
      "program": "${workspaceFolder}//Algorithms/List/loop.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    },
    {
      "name": "Launch Graph",
      "program": "${workspaceFolder}//Algorithms/Graph/jobs2.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    },
    {
      "name": "Launch Array",
      "program": "${workspaceFolder}//Algorithms/Array/mergeSort.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    },
    {
      "name": "Launch Array-DP",
      "program": "${workspaceFolder}/Algorithms/Array/DynamicProgramming/numbersInPi.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    },
    {
      "name": "Launch Array-Backtracking",
      "program": "${workspaceFolder}/Algorithms/Array/Backtracking/permutation.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    },
    {
      "name": "Launch String",
      "program": "${workspaceFolder}/Algorithms/String/wordPattern2.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    },
    {
      "name": "Launch String-DP",
      "program": "${workspaceFolder}/Algorithms/String/DynamicProgramming/interleavingString.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/Node/events/app.js",
      "outFiles": ["${workspaceFolder}/**/*.js"]
      // "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch inherit.js",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/Node/events/inherit.js",
      "outFiles": ["${workspaceFolder}/**/*.js"]
      // "console": "integratedTerminal"
    }
  ]
}
